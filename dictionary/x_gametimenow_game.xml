<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Game" max_length="40" name="x_gametimenow_game" type="collection">
        <element display="true" label="Game Date" mandatory="true" max_length="40" name="game_date" type="glide_date_time"/>
        <element choice="1" default="Sport Court" label="Location" max_length="40" name="location" type="choice">
            <choice>
                <element label="-- New choice --" sequence="0" value="-- New choice --"/>
                <element label="Sport Court" sequence="1" value="Sport Court"/>
                <element label="The Park" sequence="2" value="The Park"/>
            </choice>
        </element>
        <element label="Projected weather" max_length="40" name="projected_weather" type="string"/>
    </element>
</database>
